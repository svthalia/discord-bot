{
  "name": "thalia-bot",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "sync": "slash-up sync",
    "sync:dev": "slash-up sync -e development",
    "start": "cd dist && node index.js",
    "build": "npx tsc",
    "deploy": "yarn build && terraform apply",
    "lint": "npx eslint --ext .ts src",
    "lint:fix": "npx eslint --ext .ts src --fix",
    "fmt": "prettier ./*.{js,json} src/** --write && terraform fmt -recursive infra"
  },
  "dependencies": {
    "@discordjs/rest": "^1.1.0",
    "base64url": "^3.0.1",
    "discord-api-types": "^0.37.9",
    "dotenv": "^8.2.0",
    "slash-create": "^5.2.0",
    "undici": "^5.10.0"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.170.0",
    "@aws-sdk/lib-dynamodb": "^3.170.0",
    "@types/aws-lambda": "^8.10.103",
    "@types/express": "^4.17.11",
    "@types/node": "^18.7.17",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "prettier": "^2.2.1",
    "slash-up": "^1.0.11",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  }
}
